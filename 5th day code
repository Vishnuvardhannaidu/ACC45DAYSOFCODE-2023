#include <stdio.h>

int main()
{
    int no_of_friends,no_of_cars_reqd;
    printf("enetr no_of_friends:");
    scanf("%d",&no_of_friends);
    
    if(no_of_friends%4==0){
        printf("no_of_cars_reqd=%d",no_of_friends/4);
    }else {
        printf("no_of_cars_reqd=%d",(no_of_friends/4)+1);
    }
    
    
    

    return 0;
}
